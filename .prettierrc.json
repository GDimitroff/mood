{
  "$schema": "https://json.schemastore.org/prettierrc",
  "singleQuote": true,
  "jsxSingleQuote": false,
  "semi": false,
  "tabWidth": 2,
  "singleAttributePerLine": false,
  "printWidth": 100,
  "arrowParens": "always",
  "bracketSameLine": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "plugins": ["prettier-plugin-tailwindcss"]
}
